{
  "name": "gem-book",
  "version": "1.2.10",
  "description": "Create your document website easily and quickly",
  "keywords": [
    "doc",
    "docs",
    "documentation",
    "creator",
    "generator"
  ],
  "main": "dist/index.js",
  "module": "index.js",
  "bin": {
    "gem-book": "bin/index.js"
  },
  "typings": "index.d.ts",
  "scripts": {
    "lint": "tsc --noEmit && eslint --ext .ts --fix src/",
    "dev:cli": "webpack --watch --config webpack.config.cli.js",
    "docs": "node ./bin docs -t Gem-book -i ./logo.png --i18n --home-mode --plugins raw --debug",
    "dev:docs": "cross-env PORT=8090 nodemon --watch bin --exec \"npm run docs\"",
    "build:cli": "webpack --config webpack.config.cli.js",
    "build:ele": "tsc -p ./tsconfig.ele.json",
    "build:dist": "webpack --config webpack.config.dist.js",
    "build": "npm run build:cli && npm run build:ele && npm run build:dist",
    "build:website": "npm run build:cli && npm run docs -- --build",
    "prepublishOnly": "npm run lint && npm run build",
    "test:fe": "npm run build:ele && karma start --coverage",
    "test": "npm run test:fe"
  },
  "author": "mantou132",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mantou132/gem-book.git"
  },
  "license": "ISC",
  "dependencies": {
    "@mantou/gem": "^0.6.7",
    "commander": "^4.1.1",
    "copy-webpack-plugin": "^6.3.2",
    "front-matter": "^4.0.2",
    "git-remote-origin-url": "^3.1.0",
    "git-repo-info": "^2.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "marked": "^0.8.2",
    "mkdirp": "^0.5.5",
    "parse-github-url": "^1.0.2",
    "prismjs": "^1.22.0",
    "ts-loader": "^8.0.6",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.29",
    "@open-wc/testing-karma": "^3.4.8",
    "@open-wc/testing-karma-bs": "^1.3.94",
    "@types/copy-webpack-plugin": "^6.3.0",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.162",
    "@types/marked": "^0.7.4",
    "@types/mkdirp": "^0.5.2",
    "@types/parse-github-url": "^1.0.0",
    "@types/prismjs": "^1.16.1",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^6.0.3",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
